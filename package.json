{
  "name": "@tswaters/tiny-diff",
  "version": "0.0.4",
  "description": "Tiny diff library",
  "main": "dist/tiny-diff.js",
  "module": "dist/tiny-diff.mjs",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "pretest": "rimraf .nyc_output coverage",
    "test": "cross-env NODE_ENV=instrument nyc mocha"
  },
  "files": [
    "dist"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [],
  "author": "Tyler Waters <tyler.waters@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tswaters/tiny-diff.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.3.0",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.3"
  }
}
